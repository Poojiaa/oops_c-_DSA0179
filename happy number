#include<iostream>
using namespace std;
int main()
{
	int n,r,sum=0,m;
	cout<<"enter the number"<<" ";
	cin>>n;
	while(n!=1 && n!=4)
	{sum=0;
		
	
	while(n!=0)
	{
		r=n%10;
		sum=sum+(r*r);           
		n=n/10;
	}
		n=sum;
		}
		if(n==1)
		cout<<"it is a happy number";
		else
		cout<<"it is not a happy number";
	}
		
