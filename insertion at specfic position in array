#include<iostream>
using namespace std;
int main()
{
	int a[]={1,3,6,8,2,3,9,1,8},n,pos,ele,i,len;
	len=sizeof(a)/sizeof(a[0]);
	cout<<"enter the position"<<endl;
	cin>>pos;
	cout<<"enter the element"<<endl;
	cin>>ele;
	for(i=len-1;i>pos-1;i--)
	{
		a[i]=a[i-1];
		
	}
  a[i]=ele;
  for(i=0;i<len;i++)
  {
  	cout<<a[i]<<endl;
  }
}
