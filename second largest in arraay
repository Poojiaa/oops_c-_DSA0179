#include<iostream>
using namespace std;
int main()
{
	int a[5],len,i,j,temp,largest,n;
	
	len=sizeof(a)/sizeof(a[0]);
	for(i=0;i<len;i++)
	{ cin>>a[i];}
	for(i=0;i<len;i++)
	{
	
		for(j=j+1;j<len;j++)
		{
			if(a[i]>a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
		
		
	}
	largest=a[len-1];
	cout<<largest<<endl;
}
